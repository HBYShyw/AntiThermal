<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.coui.appcompat.preference.COUIPreferenceCategory
        android:key="power_analysis_category"
        app:isFirstCategory="true">
        <com.coui.appcompat.preference.COUIPreference
            android:title="@string/usage_type_foreground_activity"
            android:key="fore_act_time"
            android:summary="@string/usage_type_foreground_activity"
            app:isBackgroundAnimationEnabled="false"/>
        <com.coui.appcompat.preference.COUIPreference
            android:title="@string/usage_type_background_activity"
            android:key="back_act_time"
            android:summary="@string/usage_type_background_activity"
            app:isBackgroundAnimationEnabled="false"/>
        <com.coui.appcompat.preference.COUIPreference
            android:title="@string/battery_detail_view_keep_active_time"
            android:key="keep_active_time"
            android:summary="@string/battery_detail_view_keep_active_time"
            app:isBackgroundAnimationEnabled="false"/>
        <com.coui.appcompat.preference.COUIPreference
            android:title="@string/battery_detail_view_wlan_upload_stream"
            android:key="wlan_upload_stream"
            android:summary="@string/battery_detail_view_wlan_upload_stream"
            app:isBackgroundAnimationEnabled="false"/>
        <com.coui.appcompat.preference.COUIPreference
            android:title="@string/battery_detail_view_wlan_download_stream"
            android:key="wlan_download_stream"
            android:summary="@string/battery_detail_view_wlan_download_stream"
            app:isBackgroundAnimationEnabled="false"/>
        <com.coui.appcompat.preference.COUIPreference
            android:key="computed_power"
            android:summary="@string/usage_type_computed_power"
            app:isBackgroundAnimationEnabled="false"/>
    </com.coui.appcompat.preference.COUIPreferenceCategory>
    <com.coui.appcompat.preference.COUIPreferenceCategory
        android:title="@string/power_limit_title"
        android:key="power_limit_category">
        <com.coui.appcompat.preference.COUISwitchPreference
            android:persistent="false"
            android:title="@string/foreground_act_title"
            android:key="foreground_act_switch"
            android:summary="@string/foreground_act_summary"
            android:defaultValue="false"/>
        <com.coui.appcompat.preference.COUISwitchPreference
            android:persistent="false"
            android:title="@string/background_act_title"
            android:key="background_act_switch"
            android:summary="@string/background_act_summary"
            android:defaultValue="false"/>
    </com.coui.appcompat.preference.COUIPreferenceCategory>
    <com.coui.appcompat.preference.COUIPreferenceCategory
        android:title="@string/start_limit_title"
        android:key="start_limit_category">
        <com.coui.appcompat.preference.COUISwitchPreference
            android:persistent="false"
            android:title="@string/initiative_start_title"
            android:key="initiative_start_switch"
            android:defaultValue="false"/>
        <com.coui.appcompat.preference.COUISwitchPreference
            android:persistent="false"
            android:title="@string/passive_start_title"
            android:key="passive_start_switch"
            android:defaultValue="false"/>
    </com.coui.appcompat.preference.COUIPreferenceCategory>
    <com.coui.appcompat.preference.COUIPreferenceCategory android:key="force_stop_cat">
        <com.oplus.powermanager.powercurve.graph.SipperFSPreference android:key="force_stop"/>
    </com.coui.appcompat.preference.COUIPreferenceCategory>
    <com.coui.appcompat.preference.COUIPreference
        android:enabled="false"
        android:layout="@layout/bottom_layout"
        android:key="bottom"/>
</androidx.preference.PreferenceScreen>
