<?xml version="1.0" encoding="utf-8"?>
<com.coui.appcompat.dialog.widget.COUIMaxHeightNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:scrollbarThumbVertical="@drawable/coui_scrollbar_handle_vertical"
    android:scrollbarStyle="insideOverlay"
    android:orientation="vertical"
    android:scrollbars="vertical"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="1">
    <com.coui.appcompat.buttonBar.COUIButtonBarLayout
        android:orientation="horizontal"
        android:id="@+id/buttonPanel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/coui_alert_dialog_button_height"
        app:forceVertical="true"
        app:verNegButVerPaddingOffset="@dimen/alert_dialog_list_last_item_padding_offset">
        <androidx.appcompat.widget.AppCompatButton
            android:id="@android:id/button3"
            android:singleLine="false"
            style="@style/COUIAlertDialogBottomButton.Bottom.Last"/>
        <ImageView
            android:id="@+id/coui_dialog_button_divider_1"
            android:visibility="gone"
            style="@style/COUIAlertDialogBottomButtonDivider"/>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@android:id/button1"
            android:singleLine="false"
            style="@style/COUIAlertDialogBottomButton.Bottom"/>
        <ImageView
            android:id="@+id/coui_dialog_button_divider_2"
            android:visibility="gone"
            style="@style/COUIAlertDialogBottomButtonDivider"/>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@android:id/button2"
            android:singleLine="false"
            style="@style/COUIAlertDialogBottomButton.Bottom"/>
    </com.coui.appcompat.buttonBar.COUIButtonBarLayout>
</com.coui.appcompat.dialog.widget.COUIMaxHeightNestedScrollView>
