<?xml version="1.0" encoding="utf-8"?>
<com.coui.appcompat.poplist.RoundFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:forceDarkAllowed="false"
    app:rfRadius="@dimen/coui_popup_list_window_content_radius">
    <com.coui.appcompat.list.COUIForegroundListView
        android:id="@+id/coui_popup_list_view"
        android:background="@android:color/transparent"
        android:scrollbars="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/coui_popup_list_window_margin_top"
        android:layout_marginBottom="@dimen/coui_popup_list_window_margin_bottom"
        android:listSelector="@drawable/coui_popup_list_selector"
        android:divider="@null"
        android:overScrollMode="never"
        android:layout_marginStart="@dimen/support_shadow_size_level_three"
        android:layout_marginEnd="@dimen/support_shadow_size_level_three"/>
</com.coui.appcompat.poplist.RoundFrameLayout>
