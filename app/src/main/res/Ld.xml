<?xml version="1.0" encoding="utf-8"?>
<com.coui.appcompat.cardlist.COUICardListSelectedItemLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingHorizontal="@dimen/battery_ui_preference_margin_horizontal">
    <RelativeLayout
        android:layout_gravity="center"
        android:id="@+id/open_time_root"
        android:paddingBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:paddingStart="16dp"
        android:paddingEnd="16dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:ellipsize="marquee"
            android:id="@+id/aod_set_start_time"
            android:paddingTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/custom_low_power_charging_time_begin"
            android:singleLine="true"
            android:layout_alignParentStart="true"/>
        <TextView
            android:textAppearance="@style/TextAppearance.COUI.List.Assignment"
            android:textColor="?attr/couiColorPrimary"
            android:ellipsize="end"
            android:id="@+id/open_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:maxLines="10"
            android:layout_below="@+id/aod_set_start_time"
            android:layout_alignStart="@+id/aod_set_start_time"/>
    </RelativeLayout>
    <ViewStub
        android:id="@+id/time_set_open_stub"
        android:layout="@layout/local_time_picker_layout"
        android:inflatedId="@+id/open_time_picker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <ImageView
        android:background="@drawable/coui_divider_horizontal_without_padding"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/common_margin"/>
    <RelativeLayout
        android:layout_gravity="center"
        android:id="@+id/close_time_root"
        android:paddingBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="58dp"
        android:paddingStart="16dp"
        android:paddingEnd="16dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:ellipsize="marquee"
            android:id="@+id/title"
            android:paddingTop="12dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/custom_low_power_charging_time_end"
            android:singleLine="true"
            android:layout_alignParentStart="true"/>
        <TextView
            android:textAppearance="@style/TextAppearance.COUI.List.Assignment"
            android:textColor="?attr/couiColorPrimary"
            android:ellipsize="end"
            android:id="@+id/close_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:maxLines="10"
            android:layout_below="@+id/title"
            android:layout_alignStart="@+id/title"/>
    </RelativeLayout>
    <ViewStub
        android:id="@+id/time_set_close_stub"
        android:layout="@layout/local_time_picker_layout"
        android:inflatedId="@+id/close_time_picker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.coui.appcompat.cardlist.COUICardListSelectedItemLayout>
